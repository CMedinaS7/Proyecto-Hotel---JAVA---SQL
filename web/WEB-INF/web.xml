    <servlet>
        <servlet-name>moduloPersonal</servlet-name>
        <servlet-class>moduloPersonal</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BuscarDisponibilidad</servlet-name>
        <url-pattern>/BuscarDisponibilidad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrarCliente</servlet-name>
        <url-pattern>/RegistrarCliente</url-pattern>
